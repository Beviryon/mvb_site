{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": "Services immobiliers propos√©s"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "icon": {
      "type": "string",
      "required": true
    },
    "features": {
      "type": "json",
      "required": true
    },
    "price": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "home_page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::home-page.home-page",
      "inversedBy": "services"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
